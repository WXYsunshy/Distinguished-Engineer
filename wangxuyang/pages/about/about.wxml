<view class="list">
  <block wx:for="{{pic}}">
    <view class="item">
      <view class="item-left">
        <image wx:key="{{item.id}}" src='{{pic[index].src}}'></image>
      </view>
      <view class="item-middle">
        <text class="text-title">{{pic[index].title}}</text>
        <text class="text-desc">{{pic[index].desc}}
      </text>
        <text class="text-dubbing"> {{pic[index].dubbing}} </text>
      </view>
      <view class="item-right">
        <text class="text-score">{{pic[index].score}}</text>
        <button type="primary" size="mini" plain="true"> 购票 </button>
      </view>
    </view>
  </block>
</view>
